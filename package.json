{
  "name": "rpolitecnicausuarios",
  "version": "1.0.0",
  "description": "Aplicación para gestión de usuarios de Revista Politécnica",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run start:dev -w backend\" \"npm run dev -w frontend\"",
    "dev:frontend": "npm run dev -w frontend", 
    "dev:backend": "npm run start:dev -w backend",
    "build": "npm run build -w backend && npm run build -w frontend",
    "install:all": "npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jegiraldp/rpolitecnicaUsuarios.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/jegiraldp/rpolitecnicaUsuarios/issues"
  },
  "homepage": "https://github.com/jegiraldp/rpolitecnicaUsuarios#readme"
}
